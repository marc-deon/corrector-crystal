    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Debug Corrector",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/corrector_crystal",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
                
            ]
        },
        {
            "name": "Debug Asamiya",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/asamiya",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
                
            ]
        }
    ]
}